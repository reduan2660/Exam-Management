            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="75.0" layoutY="16.0" mnemonicParsing="false" onAction="#allCourse" styleClass="nav-btn" stylesheets="@../../assets/css/style.css" text="All Courses" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="profileBtn" layoutX="887.0" layoutY="16.0" mnemonicParsing="false" onAction="#viewProfile" styleClass="nav-btn" stylesheets="@../../assets/css/style.css" text="Profile" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="992.0" layoutY="16.0" mnemonicParsing="false" styleClass="nav-btn" stylesheets="@../../assets/css/style.css" text="Dashboard" textFill="WHITE">
