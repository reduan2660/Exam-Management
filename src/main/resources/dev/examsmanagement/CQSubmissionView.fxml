      <Button fx:id="submitBtn" layoutX="622.0" layoutY="426.0" mnemonicParsing="false" onAction="#submitAnswer" styleClass="btn" text="Submit">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Text fx:id="questionTitle" layoutX="247.0" layoutY="159.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Question">
